<h1>Users Index</h1>
<%= link_to 'Logout', destroy_user_session_path, method: :delete %>

 <% if flash[:success] %>
    <div class="success"><%= flash[:success] %></div>
  <% end %>

<% @users.each do |user| %>
<td><%= link_to (image_tag user.avatar.url(:baby)), user %></td>
<li class="li_trans"><%= link_to user.username, user %></li>
<% end %>


